INSTALL_PATH = /
SKIP_INSTALL = YES

PRODUCT_NAME = SalesforceSecurity
OTHER_LDFLAGS = -ObjC -lSalesforceCommonUtils $(inherited)

HEADER_SEARCH_PATHS = $(SRCROOT)/../../external/ThirdPartyDependencies/SalesforceCommonUtils/Headers $(inherited)
LIBRARY_SEARCH_PATHS = "$(SRCROOT)/../../external/ThirdPartyDependencies/SalesforceCommonUtils" $(inherited)

GCC_PRECOMPILE_PREFIX_HEADER = YES
GCC_PREFIX_HEADER = $(PROJECT_NAME)/$(PROJECT_NAME)-Prefix.pch

GCC_PRECOMPILE_PREFIX_HEADER = YES
GCC_PREFIX_HEADER = $(PROJECT_NAME)/$(PROJECT_NAME)-Prefix.pch

PRODUCT_NAME = SalesforceHybridSDK

HEADER_SEARCH_PATHS = "$(BUILT_PRODUCTS_DIR)/Headers" "$(TARGET_BUILD_DIR)/Headers" "$(PROJECT_DIR)/../../external/cordova-ios/cordova-ios/CordovaLib/Classes/**" "$(PROJECT_DIR)/../../external/ThirdPartyDependencies/SalesforceCommonUtils/Headers" $(inherited)
LIBRARY_SEARCH_PATHS = "$(SRCROOT)/../../external/ThirdPartyDependencies/openssl" "$(SRCROOT)/../../external/ThirdPartyDependencies/sqlcipher" "$(SRCROOT)/../../external/ThirdPartyDependencies/SalesforceCommonUtils" $(inherited)
